{
  "name": "@syneto/compass-react",
  "version": "1.7.1",
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "scripts": {
    "build": "run-s build:*",
    "build:test": "npm run test",
    "build:clean": "rm -rf lib",
    "build:rollup": "rollup --config",
    "build:styleguide": "npx styleguidist build",
    "develop": "npx styleguidist server",
    "format": "prettier --write",
    "lint": "npx lint-staged",
    "lint:css": "stylelint --fix",
    "lint:js": "eslint --fix",
    "lint:scss": "stylelint --fix --syntax scss",
    "release:major": "npm version major",
    "release:minor": "npm version minor",
    "release:patch": "npm version patch",
    "preversion": "npm test",
    "version": "npm run build && git add docs && git add lib && changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "test:update": "jest -u"
  },
  "lint-staged": {
    "*.css": [
      "npm run lint:css",
      "npm run format",
      "git add"
    ],
    "*.js": [
      "npm run lint:js",
      "npm run format",
      "git add"
    ],
    "*.scss": [
      "npm run lint:scss",
      "npm run format",
      "git add"
    ],
    "*.{json,md,yml}": [
      "npm run format",
      "git add"
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@trendmicro/react-sidenav": "^0.5.0",
    "bootstrap": "^4.3.1",
    "date-fns": "^2.0.0-alpha.23",
    "prop-types": "^15.7.2",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-bootstrap-table-next": "^3.3.1",
    "react-bootstrap-table2-filter": "^1.3.0",
    "react-bootstrap-table2-paginator": "^2.1.0",
    "react-bootstrap-table2-toolkit": "^2.1.0",
    "react-datepicker": "^2.10.1",
    "react-ink": "^6.4.0",
    "react-minimal-pie-chart": "^6.0.1",
    "react-switch": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "classnames": "^2.2.6",
    "clsx": "^1.0.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "generate-changelog": "^1.7.1",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.14",
    "prettier": "^1.16.4",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-select": "^3.0.4",
    "react-styleguidist": "^10.6.1",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-copy": "^2.0.1",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-sass-variables": "^0.1.1",
    "rollup-plugin-visualizer": "^1.1.1",
    "sass-loader": "^7.1.0",
    "styled-components": "^5.0.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.3.0",
    "webpack": "^4.29.6",
    "webpack-blocks": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-select": "^3.0.4"
  },
  "description": "React implementation of Syneto's Compass Design System: https://synetonet.github.io/compass-react/",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SynetoNet/compass-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SynetoNet/compass-react/issues"
  },
  "homepage": "https://synetonet.github.io/compass-react/"
}
