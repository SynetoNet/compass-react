{
  "name": "@syneto/compass-react",
  "version": "2.0.2",
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:test": "npm run test",
    "build:clean": "rm -rf lib",
    "build:rollup": "rollup --config",
    "build:styleguide": "npx styleguidist build",
    "develop": "npx styleguidist server",
    "format": "prettier --write",
    "lint": "npx lint-staged",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "lint:js": "eslint --fix",
    "lint:scss": "stylelint \"**/*.scss\" --fix",
    "release:major": "npm version major",
    "release:minor": "npm version minor",
    "release:patch": "npm version patch",
    "preversion": "npm test",
    "version": "npm run build && git add docs && git add lib && changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "test:update": "jest -u"
  },
  "lint-staged": {
    "*.css": [
      "npm run lint:css",
      "npm run format",
      "git add"
    ],
    "*.js": [
      "npm run lint:js",
      "npm run format",
      "git add"
    ],
    "*.scss": [
      "npm run lint:scss",
      "npm run format",
      "git add"
    ],
    "*.{json,md,yml}": [
      "npm run format",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@trendmicro/react-sidenav": "^0.5.0",
    "bootstrap": "^5.3.3",
    "date-fns": "^4.1.0",
    "prop-types": "^15.8.1",
    "react-bootstrap": "^2.10.5",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-filter": "^1.3.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-bootstrap-table2-toolkit": "^2.1.3",
    "react-datepicker": "^7.4.0",
    "react-ink": "^6.5.4",
    "react-minimal-pie-chart": "^8.4.0",
    "react-switch": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.20",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "file-loader": "^6.2.0",
    "generate-changelog": "^1.8.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-select": "^5.8.1",
    "react-styleguidist": "^13.1.3",
    "react-test-renderer": "^18.3.1",
    "rollup": "^2.79.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass-variables": "^0.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "style-loader": "^4.0.0",
    "styled-components": "^6.1.13",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "webpack": "^5.95.0"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-select": "^5.8.1"
  },
  "overrides": {
    "date-fns": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "description": "React implementation of Syneto's Compass Design System: https://synetonet.github.io/compass-react/",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SynetoNet/compass-react.git"
