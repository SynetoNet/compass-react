{
  "name": "@syneto/compass-react",
  "version": "1.7.19",
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "scripts": {
    "build": "run-s build:*",
    "build:test": "npm run test",
    "build:clean": "rm -rf lib",
    "build:rollup": "rollup --config",
    "build:styleguide": "npx styleguidist build",
    "develop": "npx styleguidist server",
    "format": "prettier --write",
    "lint": "npx lint-staged",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "lint:js": "eslint --fix",
    "lint:scss": "stylelint \"**/*.scss\" --fix",
    "release:major": "npm version major",
    "release:minor": "npm version minor",
    "release:patch": "npm version patch",
    "preversion": "npm test",
    "version": "npm run build && git add docs && git add lib && changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "test:update": "jest -u"
  },
  "lint-staged": {
    "*.css": [
      "npm run lint:css",
      "npm run format",
      "git add"
    ],
    "*.js": [
      "npm run lint:js",
      "npm run format",
      "git add"
    ],
    "*.scss": [
      "npm run lint:scss",
      "npm run format",
      "git add"
    ],
    "*.{json,md,yml}": [
      "npm run format",
      "git add"
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@trendmicro/react-sidenav": "^0.5.0",
    "bootstrap": "^5.1.3",
    "date-fns": "^2.28.0",
    "prop-types": "^15.8.1",
    "react-bootstrap": "^2.1.2",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-filter": "^1.3.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-bootstrap-table2-toolkit": "^2.1.3",
    "react-datepicker": "^4.6.0",
    "react-ink": "^6.5.1",
    "react-minimal-pie-chart": "^8.2.0",
    "react-switch": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "classnames": "^2.3.1",
    "clsx": "^1.0.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^6.2.0",
    "generate-changelog": "^1.7.1",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "lint-staged": "^12.3.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-select": "^5.2.2",
    "react-styleguidist": "^11.2.0",
    "react-test-renderer": "^16.8.6",
    "rollup": "^2.67.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass-variables": "^0.1.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "styled-components": "^5.0.1",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "webpack": "^5.68.0",
    "webpack-blocks": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-select": "^3.0.4"
  },
  "description": "React implementation of Syneto's Compass Design System: https://synetonet.github.io/compass-react/",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SynetoNet/compass-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SynetoNet/compass-react/issues"
  },
  "homepage": "https://synetonet.github.io/compass-react/"
}
